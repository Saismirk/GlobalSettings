*.pdb.meta
*.mdb.meta

# Unity3D generated file on crash reports
sysinfo.txt

# Builds
*.apk
*.aab
*.unitypackage
*.app

# Crashlytics generated file
src/GlobalSettings/crashlytics-build.properties

# Packed Addressables
src/GlobalSettings/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

